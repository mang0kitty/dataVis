  "version": "0.2.0",
  "configurations": [
    {
      "type": "edge",
      "request": "launch",
      "name": "Launch Edge",
      "url": "http://localhost:3000",
      "webRoot": "${fileDirname}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:3000",
      "webRoot": "${fileDirname}",
      "runtimeExecutable": "C:\\Program Files (x86)\\Microsoft\\Edge Beta\\Application\\msedge.exe",
      "runtimeArgs": ["--profile-directory=Default"]
    }
  ]
}
